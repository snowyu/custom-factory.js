{
  "name": "custom-factory",
  "version": "2.2.0",
  "description": "more easily add the factory ability to your class for node and javascript",
  "homepage": "https://github.com/snowyu/custom-factory.js",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./src/*": "./src/*.js",
    ".": {
      "require": "./lib/cjs/index.js",
      "import": "./lib/esm/index.js"
    },
    "./lib/*": {
      "require": "./lib/cjs/*.js",
      "import": "./lib/esm/*.js"
    }
  },
  "types": "./types",
  "files": [
    "README.md",
    "LICENSE-MIT",
    "index.js",
    "test",
    "src",
    "lib",
    "types"
  ],
  "scripts": {
    "build": "rimraf lib && npm run build.swc && npm run build.ts",
    "build.swc": "npm run build.swc.cjs && npm run build.swc.mjs",
    "build.swc.cjs": "swc src -d lib/cjs --config-file build.swcrc -C module.type=commonjs",
    "build.swc.mjs": "swc src -d lib/esm --config-file build.swcrc -C module.type=es6",
    "build.ts": "tsc",
    "release": "npx standard-version",
    "release.alpha": "npx standard-version --prerelease alpha",
    "start": "nps",
    "test": "jest --runInBand --config .jest.config.cjs"
  },
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "inherits-ex": "^1.3.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/node": "^7.20.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/plugin-transform-runtime": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-flow": "^7.18.6",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.40",
    "@swc/jest": "^0.2.24",
    "@types/jest": "^29.4.0",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "babel-jest": "^29.5.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-class": "^0.3.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-prettier": "^4.2.1",
    "glob": "^9.2.1",
    "jest": "^29.5.0",
    "jest-extended": "^3.2.4",
    "nps": "^5.10.0",
    "prettier": "^2.8.4",
    "rimraf": "^4.4.0",
    "typedoc": "^0.23.26",
    "typedoc-plugin-markdown": "^3.14.0",
    "typescript": "^4.9.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/snowyu/custom-factory.js"
  },
  "keywords": [
    "ability",
    "abstract",
    "factory",
    "custom",
    "custom-factory",
    "register",
    "unregister",
    "alias",
    "object",
    "class"
  ],
  "author": {
    "name": "Riceball LEE",
    "email": "snowyu.lee@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/snowyu/custom-factory.js/issues"
  }
}
