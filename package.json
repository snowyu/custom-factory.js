{
  "name": "custom-factory",
  "version": "2.2.0",
  "description": "more easily add the factory ability to your class for node and javascript",
  "homepage": "https://github.com/snowyu/custom-factory.js",
  "main": "./lib/index.js",
  "module": "./src/index.js",
  "exports": {
    "./package.json": "./package.json",
    "./src/*": "./src/*.js",
    ".": {
      "require": "./lib/cjs/index.js",
      "import": "./lib/esm/index.js"
    },
    "./lib/*": {
      "require": "./lib/cjs/*.js",
      "import": "./lib/esm/*.js"
    }
  },
  "types": "./types",
  "files": [
    "README.md",
    "LICENSE-MIT",
    "index.js",
    "test",
    "src",
    "lib",
    "types"
  ],
  "scripts": {
    "build": "rimraf lib && npm run build.cjs && npm run build.ts",
    "build.cjs": "babel src --out-dir lib --config-file ./.babelrc --ignore '**/*.spec.js' --ignore '**/*.test.js'",
    "build.ts": "tsc --declaration --emitDeclarationOnly --outDir lib",
    "release": "npx standard-version",
    "release.alpha": "npx standard-version --prerelease alpha",
    "test": "jest --runInBand --config .jest.config.cjs"
  },
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "inherits-ex": "^2.1.0-alpha.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-transform-modules-commonjs": "^7.21.2",
    "@types/jest": "^29.4.0",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "babel-jest": "^29.5.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-prettier": "^4.2.1",
    "glob": "^9.2.1",
    "jest": "^29.5.0",
    "jest-extended": "^3.2.4",
    "prettier": "^2.8.4",
    "rimraf": "^4.4.0",
    "typedoc": "^0.23.26",
    "typedoc-plugin-markdown": "^3.14.0",
    "typescript": "^4.9.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/snowyu/custom-factory.js"
  },
  "keywords": [
    "ability",
    "abstract",
    "factory",
    "custom",
    "custom-factory",
    "register",
    "unregister",
    "alias",
    "object",
    "class"
  ],
  "author": {
    "name": "Riceball LEE",
    "email": "snowyu.lee@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/snowyu/custom-factory.js/issues"
  }
}
